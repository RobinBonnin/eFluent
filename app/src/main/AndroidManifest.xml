<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.efluent">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProActivity"
            android:label="@string/title_activity_pro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PatientActivity"
            android:label="@string/title_activity_patient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddPatientActivity" />
        <activity android:name=".InscriptionProActivity" />
        <activity
            android:name=".InfoPatientActivity"
            android:label="@string/title_activity_info_patient"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>